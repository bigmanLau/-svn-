/** 首页弹窗 */
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.7;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.modal-dialog {
  width: 550rpx;
  height: 580rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 9999;
  margin: -320rpx -275rpx;
  border-radius: 0;
}
.modal-dialog-wrap{
  width: 100%;
  height: 100%;
  position: relative;
}
.modal-dialog-wrap .img{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 140rpx;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: -1;
}
