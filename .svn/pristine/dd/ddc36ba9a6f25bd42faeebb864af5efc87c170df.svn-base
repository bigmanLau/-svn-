}
.pickbox{
  min-height: 500rpx;
  position: fixed;
  bottom: 0;
  width: 100%;
  background: white;
}
.pick-title{
  height: 80rpx;
  width: 100%;
  border-top: 1rpx solid #f5f5f5;
   border-bottom: 1rpx solid #f5f5f5;
}
.cover-shadow{
  background:rgba(0,0,0,.3);
   position: absolute;
   top: 0;
   height: 100vh;
  width: 100%;
}
