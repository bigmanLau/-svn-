/**app.wxss**/
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    -webkit-text-size-adjust:none;
    -webkit-user-select: none;
    font-size: 35rpx;
    margin-top: 20rpx;
    color: #3c3c3c;
    font-family: Helvetica,Arial,"Helvetica Neue","Droid Sans","Microsoft YaHei",sans-serif;
}
.canvas {
    width: 100%;
    height: 200px;
}
.container-menu{
  width: calc(100% - 64rpx);
  height: 40rpx;
  display: flex;
  justify-content: flex-start;
  align-items: stretch;
  box-sizing: border-box;
  margin: 16rpx 32rpx 30rpx;
  border-radius: 10rpx;
  overflow: hidden;
  background: RGBA(247, 247, 247, 1);
}
.container-menu-item{
  flex: 1;
  font-size: 24rpx;
  text-align: center;
  line-height: 40rpx;
  background: RGBA(247, 247, 247, 1);
  border-radius: 10rpx;
}
.container-menu-item.tag{
  background: RGBA(42, 158, 241, 1);
  color: #fff;
}

.center-set{
  height: 30rpx;
  width: 30rpx;
  display: inline-block;
  vertical-align: top;
  margin-left: 10rpx;
}
.center-set-text{
  display: inline-block;
  font-size: 30rpx;
  line-height: 1;
  width: 250rpx;
  vertical-align: top;
}
.shadow-border{
  width:710rpx;
min-height:330rpx;
background:rgba(255,255,255,1);
border-radius:10rpx;
box-shadow:13rpx 0rpx 38rpx rgba(14,138,227,0.1);
}
