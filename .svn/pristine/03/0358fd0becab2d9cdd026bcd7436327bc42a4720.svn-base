/* pages/zizhanghao/index.wxss */
page {
  background: #f2f2f2;

}

.container {
  background-color: #f2f2f2;
  width: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.widthatuo{
  min-width: 40rpx;
  width: 100rpx;
}
.tab{
  height: 57rpx;
  width: 248rpx;
  display: flex;
  justify-content: center;
  align-items: center;

}
.left_circle{
  border-top-left-radius: 57rpx;
  border-bottom-left-radius: 57rpx;
}
.left_border{
  border-radius: 1rpx solid #2A9EF1;

}
.right_circle{
  border-top-right-radius: 57rpx;
  border-bottom-right-radius: 57rpx;
}
.blue_border{
  border: 1rpx solid #2A9EF1;
  
}
.top-tab {
  width: 550rpx;
  height: 60rpx;
  border-radius: 10rpx;
  overflow: hidden;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  background: #fff;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.top-tab-item {
  flex: 1;
  height: 60rpx;
  color: #41a4f9;
  font-size: 30rpx;
  box-sizing: border-box;
  border: 1px solid #41a4f9;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.top-tab-item:first-child{
  border-radius: 10rpx 0 0 10rpx;
}
.top-tab-item:last-child{
  border-radius: 0 10rpx 10rpx 0;
}
.select-one {
  background: #41a4f9;
  color: #fff;
}

.container-view{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.container-view-item{
  width: calc(100% - 64rpx);
  margin: 20rpx 32rpx;
  padding: 20rpx 20rpx;
  box-sizing: border-box;
  background: #fff;
  border-radius: 10rpx;
  box-shadow: 0 0 10rpx RGBA(145, 145, 145, 0.3);
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  justify-content: flex-start;
}
.container-view-item-left,.container-view-item-right{
 flex: 1;
}
.container-view-item-left{
  text-align: left;
}
.container-view-item-right{
  text-align: right;
}
.container-view-item .title{
  font-size: 30rpx;
  color: RGBA(89, 89, 89, 1);
  line-height: 1;
  padding: 8rpx 0 12rpx;
  vertical-align: top;
}
.container-view-item .title .text{
  display: inline-block;
  height: 30rpx;
  width: 50rpx;
  vertical-align: top;
  padding: 0 10rpx;
}
.container-view-item .title .text .img{
  width: 100%;
  height: auto;
}
.container-view-item .item{
  font-size: 0;
}
.container-view-item .item text{
  font-size: 24rpx;
  color: RGBA(112, 112, 112, 1);
  line-height: 1;
  padding: 12rpx 20rpx 8rpx;
  display: inline-block;
}
.text{
    font-size: 24rpx;
  color: RGBA(112, 112, 112, 1);
  line-height: 1;
  padding: 12rpx 20rpx 8rpx;
  display: inline-block;
}
.container-view-item .item text:first-child{
  padding-left: 0;
}
.container-view-item .item text:last-child{
  padding-right: 0;
}
.container-view-item .item text.tag{
  color: RGBA(42, 158, 241, 1);
}
.container-view-button{
  width: 390rpx;
  height: 90rpx;
  margin: 70rpx auto 40rpx;
  box-sizing: border-box;
  border: 4rpx solid RGBA(42, 158, 241, 1);
  border-radius: 10rpx;
  color: RGBA(42, 158, 241, 1);
  font-size: 30rpx;
  line-height: 90rpx;
  text-align: center;
}
.container-view-table{
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-top: 31rpx;
}
.container-view-table-tr{
  display: flex;
  height: auto;
  overflow: hidden;
  align-items: flex-start;
  justify-content: flex-start;
}
.container-view-table-td{
  background: #2A9EF1;
}
.container-view-table-tr .container-view-table-td {
  flex: 1;
  font-size: 24rpx;
  color: RGBA(102, 102, 102, 1);

  text-align: center;
  line-height: 80rpx;
  box-sizing: border-box;
  margin-bottom: 15rpx;
}
.bluetx{
  color: #2A9EF1;
}

.container-view-table-tr .container-view-table-td.time
{
  flex: 0 0 230rpx;
}
.container-view-table-tr .container-view-table-td.name{
  flex: 0 0 130rpx;
}
.container-view-table-tr .container-view-table-td.phone{
  flex: 0 0 270rpx;
}
.container-view-table-tr .container-view-table-td.handle{
  flex: 0 0 120rpx;
}
.container-view-table-tr:first-child .container-view-table-td{
  line-height: 50rpx;

  color: white;
  margin: 0;
}
.container-view-table-td.tag{
  color: RGBA(42, 158, 241, 1);
}

