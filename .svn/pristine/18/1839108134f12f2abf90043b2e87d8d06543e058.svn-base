/* pages/yuyuekaihuyindao/index.wxss */
page{
  background: #f5f5f5;
}
.container{
  position: fixed;
  top: 32rpx;
  right: 32rpx;
  bottom: 32rpx;
  left: 32rpx;
  background: #fff;
  border-radius: 10rpx;
  box-shadow: 0 0 15rpx RGBA(130, 130, 130, 0.3);
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.container-top{
  width: 100%;
  padding-top: 20rpx;
  padding-bottom: 15rpx;
}
.container-top .container-title{
 width: 100%;
 font-size: 60rpx;
 color: RGBA(119, 119, 119, 1);
 line-height: 100rpx;
 text-align: center;
}
.container-top .container-info{
  width: 100%;
  color: RGBA(153, 153, 153, 1);
  font-size: 30rpx;
  text-align: center;
  line-height: 50rpx;
}
.container-bottom .container-info{
  width: 100%;
  color: RGBA(153, 153, 153, 1);
  font-size: 30rpx;
  text-align: center;
  line-height: 50rpx;
}
.container-button{
  width: 400rpx;
  height: 90rpx;
  line-height: 90rpx;
  border: 2px solid RGBA(42, 158, 241, 1);
  text-align: center;
  margin: 80rpx auto;
  font-size: 36rpx;
  border-radius: 10rpx;
  color: RGBA(42, 158, 241, 1);
}
.container-center{
  width: 100%;
}
.container-center-item{
  width: 100%;
  height: auto;
  overflow: hidden;
  box-sizing: border-box;
  padding: 20rpx 40rpx;
  padding-left: 50rpx;
  font-size: 0;
  margin: 25rpx 0;
}
.item-img-wrop{
  vertical-align: top;
  display: inline-block;
  width: 130rpx;
  height: 130rpx;
  background: RGBA(42, 158, 241, .15);
  border-radius: 50%;
  margin: 0 50rpx;
  box-sizing: border-box;
  padding: 15rpx;
}
.item-img-wrop-content{
  display: flex;
  width: 100%;
  height: 100%;
  background: RGBA(42, 158, 241, 1);
  border-radius: 50%;
  justify-content: center;
  align-items: center;
}
.item-img-wrop-content .img{
  width: 66rpx;
  height: auto;
  margin: auto;
}
.item-img-wrop-content .img image{
  width: 100%;
}
.item-info-wrop{
  vertical-align: top;
  display: inline-block;
  width: calc(100% - 240rpx);
}
.item-info-wrop-title{
  padding: 10rpx 0 0;
  font-size: 48rpx;
  color: RGBA(119, 119, 119, 1);
}
.item-info-wrop-info{
  font-size: 20rpx;
  color: RGBA(188, 188, 188, 1);
  line-height: 1.5;
  padding: 10rpx 0;
}
.item-info-wrop-info text{
  color: RGBA(42, 158, 241, 1);
  text-decoration: underline;
}